// Non-strict version:
// Note that this does not strongly enforce that the commitment is
// in the field.

import "EMBED/unpack" as unpack

def main(field i) -> (field[256]):

    bool[254] b = unpack(i)

    field[254] ret = [0; 254]

    for field i in 0..254 do
    	ret[i] = if b[i] then 1 else 0 fi
    endfor

	return [0, 0, ...ret]
