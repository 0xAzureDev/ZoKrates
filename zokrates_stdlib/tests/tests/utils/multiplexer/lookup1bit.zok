import "utils/multiplexer/lookup1bit" as lookup

def left() -> (field):
	bool sel = false  //left
	field[2] t  = [0, 1]
	
	0 == lookup(sel, t)
	
	return 1
	
def right() -> (field):
	bool sel = true  //right
	field[2] t = [0, 1]
	
	1 == lookup(sel, t)
	
	return 1
	
def main() -> (field):

	1 == left()
	1 == right()

	return 1