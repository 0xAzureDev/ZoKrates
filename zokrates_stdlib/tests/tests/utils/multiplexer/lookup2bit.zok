import "utils/multiplexer/lookup2bit" as lookup

def first() -> (field):
	bool[2] sel = [false, false]
	field[4] t  = [0, 1, 2, 3]
	
	0 == lookup(sel, t)
	
	return 1
	
def second() -> (field):
	bool[2] sel = [true, false]
	field[4] t  = [0, 1, 2, 3]
	
	1 == lookup(sel, t)
	
	return 1
	
def third() -> (field):
	bool[2] sel = [false, true]
	field[4] t  = [0, 1, 2 , 3]
	
	2 == lookup(sel, t)
	
	return 1

def fourth() -> (field):
	bool[2] sel = [true, true]
	field[4] t  = [0, 1, 2 , 3]
	
	3 == lookup(sel, t)
	
	return 1
	
def main() -> (field):

	1 == first()
	1 == second()
	1 == third()
	1 == fourth()
	
	return 1