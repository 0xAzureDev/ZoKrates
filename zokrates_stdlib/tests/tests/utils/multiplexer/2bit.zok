import "utils/multiplexer/2bit" as multiplex

def left() -> (field):
	bool bit = false  //left
	bool[2] a  = [false, true]
	bool[2] b = [true, false]
	
	bool[2] output = [false, true]
	output == multiplex(bit, a, b)
	
	return 1
	
def right() -> (field):
	bool bit = true  //right
	bool[2] a  = [false, true]
	bool[2] b = [true, false]
	
	bool[2] output = [true, false]
	output == multiplex(bit, a, b)
	
	return 1
	
def main() -> (field):

	1 == left()
	1 == right()

	return 1