import "utils/multiplexer/2bit" as multiplex

def left() -> (bool):
	bool bit = false  //left
	bool[2] a  = [false, true]
	bool[2] b = [true, false]
	
	bool[2] output = [false, true]
	assert(output == multiplex(bit, a, b))
	
	return true
	
def right() -> (bool):
	bool bit = true  //right
	bool[2] a  = [false, true]
	bool[2] b = [true, false]
	
	bool[2] output = [true, false]
	assert(output == multiplex(bit, a, b))
	
	return true
	
def main() -> ():

	assert(left())
	assert(right())

	return