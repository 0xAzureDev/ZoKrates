from "utils/casts.zok" import cast;

// field -> u8
// field -> u16
// field -> u32
// field -> u64

def main() {
    // field -> u8
    u8 v0 = cast(18f);
    assert(v0 == 0x12);

    // field -> u16
    u16 v1 = cast(4660f);
    assert(v1 == 0x1234);

    // field -> u32
    u32 v2 = cast(305419896f);
    assert(v2 == 0x12345678);

    // field -> u64
    u64 v3 = cast(1311768465173141112f);
    assert(v3 == 0x1234567812345678);
    return;
}