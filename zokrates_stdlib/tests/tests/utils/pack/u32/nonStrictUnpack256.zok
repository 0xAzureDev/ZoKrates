import "utils/pack/u32/nonStrictUnpack256" as unpack256

def testFive() -> (field):

	u32[8] b = unpack256(5)

	b == [0x00000000, 0x00000000, 0x00000000, 0x00000000, 0x00000000, 0x00000000, 0x00000000, 0x00000005]

	return 1

def testZero() -> (field):

	u32[8] b = unpack256(0)

	b == [0x00000000, 0x00000000, 0x00000000, 0x00000000, 0x00000000, 0x00000000, 0x00000000, 0x00000000]

	return 1

def testLarge() -> (field):

	u32[8] b = unpack256(14474011154664524427946373126085988481658748083205070504932198000989141204991)

	b == [0x1fffffff, 0xffffffff, 0xffffffff, 0xffffffff, 0xffffffff, 0xffffffff, 0xffffffff, 0xffffffff]

	return 1

def testMax() -> (field):

	u32[8] b = unpack256(21888242871839275222246405745257275088548364400416034343698204186575808495616)

	b == [0x30644e72, 0xe131a029, 0xb85045b6, 0x8181585d, 0x2833e848, 0x79b97091, 0x43e1f593, 0xf0000000]

	return 1

 def main() -> (field):

	1 == testFive()
	1 == testMax()
	1 == testZero()
	1 == testLarge()
	return 1
