import "utils/pack/u32/pack128" as pack128

def testFive() -> (field):

	u32[4] b = [0x00000000, 0x00000000, 0x00000000, 0x00000005]
	field n = pack128(b)

	5 == n

	return 1

def testZero() -> (field):

	u32[4] b = [0x00000000, 0x00000000, 0x00000000, 0x00000000]
	field n = pack128(b)

	0 == n

	return 1

def testMax() -> (field):

	u32[4] b = [0xffffffff, 0xffffffff, 0xffffffff, 0xffffffff]
	field n = pack128(b)

	2**128 - 1 == n

	return 1

 def main() -> (field):

	1 == testFive()
	1 == testMax()
	1 == testZero()

	return 1
