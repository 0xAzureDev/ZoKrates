{
  "name": "zokrates-js",
  "main": "index.js",
  "author": "Darko Macesic <darem966@gmail.com>",
  "version": "1.0.21",
  "keywords": [
    "zokrates",
    "wasm-bindgen",
    "zksnarks"
  ],
  "license": "GPLv3",
  "files": [
    "pkg",
    "index.js",
    "index.d.ts",
    "utils.js",
    "stdlib.json",
    "README.md"
  ],
  "types": "index.d.ts",
  "scripts": {
    "wasm-pack": "wasm-pack build --out-name index --release --target $1",
    "prewasm-pack": "rimraf pkg && npm run setup",
    "postwasm-pack": "rimraf pkg/README.md",
    "setup": "npm install && gulp stdlib",
    "build": "npm run wasm-pack bundler",
    "build:node": "rimraf dist-node && npm run wasm-pack nodejs && gulp node:build",
    "pretest": "npm run setup",
    "test": "mocha --require esm --recursive tests"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "dree": "^2.4.14",
    "esm": "^3.2.25",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-cli": "^2.2.0",
    "gulp-replace": "^1.0.0",
    "mocha": "^7.1.1",
    "rimraf": "^3.0.2",
    "serve": "^11.3.0",
    "text-encoding": "^0.7.0"
  },
  "dependencies": {}
}
