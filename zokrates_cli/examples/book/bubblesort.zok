def bubblesort<N>(u32[N] a) -> u32[N]:
	for u32 i in 0..(N-1) do
		for u32 j in 0..(N-1-i) do
			bool swap = a[j + 1] < a[j]
			u32 tmp = a[j]
			a[j] = if swap then a[j + 1] else a[j] fi
			a[j + 1] = if swap then tmp else a[j + 1] fi
		endfor
	endfor
	return a

def main(u32[10] a) -> u32[10]:
        return bubblesort(a)
